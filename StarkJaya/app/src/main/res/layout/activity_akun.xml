<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Akun">

  <ImageView
      android:id="@+id/imageView11"
      android:layout_width="0dp"
      android:layout_height="30dp"
      android:background="@drawable/back_color"
      android:backgroundTint="#000000
"
      app:layout_constraintBottom_toTopOf="@+id/imginfoakun"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent" />

  <ImageView
      android:id="@+id/imageView4"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:background="#000000"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/hitam" />

  <ImageView
      android:id="@+id/imageView5"
      android:layout_width="50dp"
      android:layout_height="70dp"
      android:layout_marginTop="32dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="@+id/imageView4"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/logoakun" />

  <TextView
      android:id="@+id/tvnamaakun"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="24dp"
      android:text="Nama Akun"
      android:textColor="#92DD3A"
      app:layout_constraintEnd_toEndOf="@+id/imageView4"
      app:layout_constraintStart_toStartOf="@+id/imageView4"
      app:layout_constraintTop_toBottomOf="@+id/imageView5" />

  <ImageView
      android:id="@+id/imgketerangan"
      android:layout_width="0dp"
      android:layout_height="70dp"
      android:layout_marginTop="100dp"
      android:background="@drawable/back_color"
      android:backgroundTint="#C4C4C4"
      android:onClick="imgpengaturan"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView4" />

  <ImageView
      android:id="@+id/imginfoakun"
      android:layout_width="0dp"
      android:layout_height="70dp"
      android:layout_marginTop="95dp"
      android:background="@drawable/back_color"
      android:backgroundTint="#C4C4C4"
      android:onClick="imginfoakun"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imgketerangan" />

  <ImageView
      android:id="@+id/imageView8"
      android:layout_width="0dp"
      android:layout_height="30dp"
      android:background="@drawable/back_color"
      android:backgroundTint="#000000
"
      app:layout_constraintBottom_toTopOf="@+id/imgketerangan"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent" />

  <TextView
      android:id="@+id/textView4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="300dp"
      android:layout_marginRight="300dp"
      android:layout_marginBottom="8dp"
      android:text="Pengaturan"
      android:textColor="#8BC34A"
      app:layout_constraintBottom_toTopOf="@+id/imgketerangan"
      app:layout_constraintEnd_toEndOf="@+id/imageView8"
      app:layout_constraintStart_toStartOf="@+id/imageView8" />

  <ImageView
      android:id="@+id/imglogoakun"
      android:layout_width="50dp"
      android:layout_height="70dp"
      android:layout_marginStart="16dp"
      android:layout_marginLeft="16dp"
      android:onClick="imglogoakun"
      app:layout_constraintBottom_toBottomOf="@+id/imginfoakun"
      app:layout_constraintStart_toStartOf="@+id/imginfoakun"
      app:layout_constraintTop_toBottomOf="@+id/imageView11"
      app:srcCompat="@drawable/logoakun" />

  <TextView
      android:id="@+id/tvpengaturanakun"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="32dp"
      android:layout_marginLeft="32dp"
      android:layout_marginEnd="240dp"
      android:layout_marginRight="240dp"
      android:layout_marginBottom="24dp"
      android:onClick="tvpengaturanakun"
      android:text="Pengaturan Akun"
      android:textColor="#292B28"
      app:layout_constraintBottom_toBottomOf="@+id/imgketerangan"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/imglogoakun" />

  <ImageView
      android:id="@+id/imgnext"
      android:layout_width="36dp"
      android:layout_height="18dp"
      android:layout_marginStart="200dp"
      android:layout_marginLeft="200dp"
      android:layout_marginTop="60dp"
      android:layout_marginEnd="8dp"
      android:layout_marginRight="8dp"
      android:onClick="imgnext"
      app:layout_constraintEnd_toEndOf="@+id/imgketerangan"
      app:layout_constraintStart_toEndOf="@+id/tvpengaturanakun"
      app:layout_constraintTop_toTopOf="@+id/imageView8"
      app:srcCompat="@drawable/next" />

  <ImageView
      android:id="@+id/imgsetting"
      android:layout_width="50dp"
      android:layout_height="65dp"
      android:layout_marginStart="16dp"
      android:layout_marginLeft="16dp"
      android:onClick="imgsetting"
      app:layout_constraintBottom_toBottomOf="@+id/imgketerangan"
      app:layout_constraintStart_toStartOf="@+id/imgketerangan"
      app:layout_constraintTop_toTopOf="@+id/imgketerangan"
      app:srcCompat="@drawable/setting" />

  <TextView
      android:id="@+id/tvinfoakun"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="5dp"
      android:layout_marginLeft="5dp"
      android:layout_marginEnd="250dp"
      android:layout_marginRight="250dp"
      android:layout_marginBottom="24dp"
      android:onClick="tvinfoakun"
      android:text="Info Akun"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="@+id/imginfoakun"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/imglogoakun" />

  <ImageView
      android:id="@+id/imgselanjutnya"
      android:layout_width="36dp"
      android:layout_height="18dp"
      android:layout_marginStart="200dp"
      android:layout_marginLeft="200dp"
      android:layout_marginTop="28dp"
      android:onClick="imgselanjutnya"
      app:layout_constraintEnd_toEndOf="@+id/imginfoakun"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toEndOf="@+id/tvinfoakun"
      app:layout_constraintTop_toBottomOf="@+id/imageView11"
      app:srcCompat="@drawable/next" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="24dp"
      android:layout_marginLeft="24dp"
      android:layout_marginEnd="300dp"
      android:layout_marginRight="300dp"
      android:layout_marginBottom="8dp"
      android:text="Informasi Akun"
      android:textColor="#8BC34A"
      app:layout_constraintBottom_toBottomOf="@+id/imageView11"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <ImageView
      android:id="@+id/imageView20"
      android:layout_width="0dp"
      android:layout_height="70dp"
      android:layout_marginBottom="-12dp"
      android:background="@drawable/back_color"
      android:backgroundTint="#C4C4C4"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent" />

  <ImageView
      android:id="@+id/imageView12"
      android:layout_width="29dp"
      android:layout_height="36dp"
      android:layout_marginEnd="40dp"
      android:layout_marginRight="40dp"
      android:layout_marginBottom="10dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="@+id/imageView20"
      app:srcCompat="@drawable/logoakun" />

  <TextView
      android:id="@+id/textView11"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="38dp"
      android:layout_marginRight="38dp"
      android:layout_marginBottom="8dp"
      android:text="Akun"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="@+id/imageView20"
      app:layout_constraintTop_toBottomOf="@+id/imageView12" />

  <ImageView
      android:id="@+id/imgberanda"
      android:layout_width="29dp"
      android:layout_height="36dp"
      android:layout_marginStart="40dp"
      android:layout_marginLeft="40dp"
      android:layout_marginBottom="10dp"
      android:onClick="imgberanda"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="@+id/imageView20"
      app:layout_constraintTop_toTopOf="@+id/imageView20"
      app:srcCompat="@drawable/home" />

  <TextView
      android:id="@+id/tvberanda"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="28dp"
      android:layout_marginLeft="28dp"
      android:layout_marginBottom="10dp"
      android:onClick="tvberanda"
      android:text="Beranda"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="@+id/imageView20"
      app:layout_constraintTop_toBottomOf="@+id/imgberanda" />

  <ImageView
      android:id="@+id/imageView9"
      android:layout_width="30dp"
      android:layout_height="35dp"
      android:layout_marginTop="8dp"
      app:layout_constraintEnd_toStartOf="@+id/imageView12"
      app:layout_constraintStart_toEndOf="@+id/imgberanda"
      app:layout_constraintTop_toTopOf="@+id/imageView20"
      app:srcCompat="@drawable/tambah" />

  <TextView
      android:id="@+id/textView10"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="8dp"
      android:layout_marginRight="8dp"
      android:layout_marginBottom="2dp"
      android:text="Tambah"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/textView11"
      app:layout_constraintStart_toEndOf="@+id/tvberanda" />

</androidx.constraintlayout.widget.ConstraintLayout>